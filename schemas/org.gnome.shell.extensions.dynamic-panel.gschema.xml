<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema id="org.gnome.shell.extensions.dynamic-panel" path="/org/gnome/shell/extensions/dynamic-panel/">
		<key type="i" name="radius-times">
			<default>100</default>
			<summary>corner radius times</summary>
			<description/>
		</key>
		<key type="i" name="float-width">
			<default>99</default>
			<summary>panel width in floatting mode</summary>
			<description/>
		</key>
		<key type="i" name="float-align">
			<default>1</default>
			<summary>panel align for floatting mode</summary>
			<description/>
		</key>
		<key type="i" name="top-margin">
			<default>10</default>
			<summary>the top margin of panel floatting</summary>
			<description/>
		</key>
		<key type="i" name="side-margin">
			<default>10</default>
			<summary>the side margin of panel floatting</summary>
			<description/>
		</key>
		<key type="i" name="transparent">
			<default>75</default>
			<summary>the transparent of panel floatting</summary>
			<description/>
		</key>
		<key type="b" name="transparent-menus">
			<default>true</default>
			<summary>the transparent applie of panel floatting menus</summary>
			<description/>
		</key>
		<key type="b" name="transparent-menus-keep-alpha">
			<default>false</default>
			<summary>Force menu transparency to remain</summary>
			<description/>
		</key>
		<key type="i" name="duration">
			<default>250</default>
			<summary>The rate of panel transition speed</summary>
			<description/>
		</key>
		<key type="s" name="dark-bg-color">
			<default>'rgb(0, 0, 0)'</default>
			<summary>The background Color string for dark mode</summary>
			<description/>
		</key>
		<key type="s" name="dark-fg-color">
			<default>'rgb(240, 240, 240)'</default>
			<summary>The foreground Color string for dark mode</summary>
			<description/>
		</key>
		<key type="s" name="light-bg-color">
			<default>'rgb(250, 250, 250)'</default>
			<summary>The background Color string for light mode</summary>
			<description/>
		</key>
		<key type="s" name="light-fg-color">
			<default>'rgb(0, 0, 0)'</default>
			<summary>The foreground Color string for light mode</summary>
			<description/>
		</key>
		<key type="b" name="colors-use-in-static">
			<default>false</default>
			<summary>The Colors use in static mode or not </summary>
			<description/>
		</key>
		<key type="i" name="detection-mode">
			<default>0</default>
			<summary>How to decide to enter and exit solid mode</summary>
			<description/>
		</key>
		<key type="i" name="background-mode">
			<default>0</default>
			<summary>Fill or Area background</summary>
			<description/>
		</key>
		<key type="b" name="auto-width">
			<default>false</default>
			<summary>Auto width or not</summary>
			<description/>
		</key>
		<key type="i" name="solid-type">
			<default>0</default>
			<summary>Indicates whether solid mode is docked or hidden</summary>
			<description/>
		</key>
	</schema>
</schemalist>
